#!/bin/bash

mvn -q clean package

if [ $? -eq 0 ]; then
  echo git add -A
  git commit -m "Resolve test"
  git merge step1-test2

  printf '\033[0;32m'
  echo ""
  echo ""
  echo "========================"
  echo "===  STEP 1 - Test 2 ==="
  echo "========================"
  echo ""
  echo ""
printf '\033[0m'
else
    echo  "- ‚ùå  There are test failure use ./skip to move forward anyway"
fi
